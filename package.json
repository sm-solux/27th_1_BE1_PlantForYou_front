      "@vue/standard"
    ],
    "globals": {
      "defineProps": "readonly",
      "defineEmits": "readonly"
    },
    "parserOptions": {
      "parser": "@babel/eslint-parser"
    },
    "rules": {
      "space-before-function-paren": "off"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ]
